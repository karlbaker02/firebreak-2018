<h1 class="heading-large">Create your own temporary register</h1>

<h2>2. Give your register a description</h2>

<%= form_tag("/data/save_description", method: ":post") do %>
    <div class="form-group">
      <%= text_field_tag :register_description %>
    </div>

    <div class="form-group">
      <%= submit_tag "Continue", {class: "button"} %>
    </div>

<% end %>




